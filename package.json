{
  "name": "crowdingless",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "predeploy": "rm -rf node_modules/sharp && yarn add --platform=linux --arch=x64 --target=12.x sharp",
    "deploy": "serverless deploy -v",
    "postdeploy": "rm -rf node_modules/sharp && yarn add sharp",
    "prewindeploy": "rd /q /s node_modules & yarn add --platform=linux --arch=x64 --target=12.x sharp",
    "windeploy": "serverless deploy -v",
    "postwindeploy": "rd /q /s node_modules & yarn add sharp",
    "predevwindeploy": "rd /q /s node_modules & yarn add --platform=linux --arch=x64 --target=12.x sharp",
    "devwindeploy": "serverless deploy -v --stage dev",
    "postdevwindeploy": "rd /q /s node_modules & yarn add sharp",
    "logs": "serverless logs -f list --stage dev"
  },
  "repository": "https://github.com/joaobispo2077/crowdingless.git",
  "author": "Jo√£o Bispo <vitor.vi10@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "sharp": "^0.26.2"
  }
}
